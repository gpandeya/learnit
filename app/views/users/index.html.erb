
    <% if @message %>
    <div id="error_explanation" class="border border-danger rounded p-2"> <%=@message %></div>
    <% end %>


   
    <div>
        <h4>Registered Profiles</h4>
    </div>
    <div >
        <table class="table" >
            
        <thead>
            <tr>
                <th></th>
                 <th> Id</th>
                <th> Email</th>
                <th> Role</th>
                
            </tr>
        </thead>
        <tbody>
            <% @users.each do |u|%>
            <tr>
              
                <td>
                    <%= link_to 'Edit', user_path(u)%>
                </td>
                <td><%= u.id %></td>
                <td><%= u.email %></td>
                <td><%= u.role %></td>
                
            </tr>
            <% end %>
        </tbody>
        </table>
    </div>


